# enumerate.hosts
set enumerate.hosts.output caplets/enumerate/hosts/hosts.log

# events.stream
events.stream off
set events.stream.output caplets/enumerate/hosts/events.stream.output
events.stream on

# net.sniff
set net.sniff.verbose false
net.sniff on

# http.proxy
set http.proxy.script caplets/enumerate/hosts/module.js
set http.proxy.port 8022
http.proxy on
